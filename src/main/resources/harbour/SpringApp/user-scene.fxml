<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="userPane" prefHeight="323.0" prefWidth="308.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="harbour.SpringApp.UserController">
   <children>
      <Button layoutX="94.0" layoutY="62.0" mnemonicParsing="false" onAction="#addUser" prefHeight="25.0" prefWidth="120.0" text="Добавить" />
      <Button layoutX="94.0" layoutY="111.0" mnemonicParsing="false" onAction="#findUser" prefHeight="25.0" prefWidth="120.0" text="Найти" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToPreviousWindow" prefHeight="25.0" prefWidth="73.0" text="Назад">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="94.0" layoutY="162.0" mnemonicParsing="false" onAction="#removeUser" prefHeight="25.0" prefWidth="120.0" text="Удалить" />
      <Button layoutX="96.0" layoutY="208.0" mnemonicParsing="false" onAction="#findAllUser" prefHeight="25.0" prefWidth="120.0" text="Все пользователи" />
   </children>
</AnchorPane>
