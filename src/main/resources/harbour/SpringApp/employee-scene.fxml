<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="pane" prefHeight="324.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="harbour.SpringApp.EmployeeController">
   <children>
      <Button layoutX="104.0" layoutY="72.0" mnemonicParsing="false" onAction="#addEmployee" prefHeight="25.0" prefWidth="120.0" text="Добавить" />
      <Button layoutX="104.0" layoutY="121.0" mnemonicParsing="false" onAction="#findEmployee" prefHeight="25.0" prefWidth="120.0" text="Найти" />
      <Button fx:id="backButton" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#backToPreviousWindow" prefHeight="25.0" prefWidth="73.0" text="Назад">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="104.0" layoutY="172.0" mnemonicParsing="false" onAction="#removeEmployee" prefHeight="25.0" prefWidth="120.0" text="Удалить" />
      <Button layoutX="106.0" layoutY="218.0" mnemonicParsing="false" onAction="#findAllEmployee" prefHeight="25.0" prefWidth="120.0" text="Все сотрудники" />
   </children>
</AnchorPane>
